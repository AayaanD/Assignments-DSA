#include<iostream>
using namespace std;
int main()
{
    int arr[100]; int n;
    cout<<"Enter size of array:\n";
    cin>>n;
    cout<<"Enter elements:\n";
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int s=0;
    int e=n-1;
   while(s<e)
   {
    swap(arr[s],arr[e]);
    s++;
    e--;
   }
   cout<<"After Reversing Elements\n";
   for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}
